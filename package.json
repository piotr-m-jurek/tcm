    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "tsx": "^4.19.2",
    "typescript-eslint": "^8.12.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@std/csv": "npm:@jsr/std__csv",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.0",
    "eslint": "^9.14.0"
  }
